FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     interFoam;

//startFrom       startTime;
startFrom       latestTime;

startTime       0;

//stopAt          endTime;
stopAt          nextWrite;

endTime         100.0;

deltaT          0.01;

writeControl    adjustableRunTime;

writeInterval   100;

purgeWrite      0;

writeFormat     acsii ;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  on;

maxCo           1;
maxAlphaCo      1;

maxDeltaT       1;


