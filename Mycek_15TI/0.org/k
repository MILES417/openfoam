FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

#include        "include/initialConditions"

internalField   uniform $turbulentKE;

boundaryField
{
    In 
    {
        type            fixedValue;
        value           uniform $turbulentKE;
    }
    
    aboveIn 
    {
        type            fixedValue;
        value           uniform $turbulentKE;
    }
    
    Out
    {
         type            zeroGradient;
    }
    
    aboveOut
    {
         type            zeroGradient;
    }
    
    atmosphere
    {
         type            zeroGradient;
    }
    
    sides
    {
        type            slip;
    }
    defaultFaces
    {
        type            empty;
    }

}


// ************************************************************************* //
